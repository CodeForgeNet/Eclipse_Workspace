package com.nestedClass;

import java.util.Scanner;

public class University {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter length of arr : ");
		int n = sc.nextInt();
		int[] sqrArr = new int[n];
		for(int i=1; i<=n; i++) {
			sqrArr[i-1] = i*i;
		}
		for(int a : sqrArr) {
			System.out.print(a+" ");
		}
	}
}
