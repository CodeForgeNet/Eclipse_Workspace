package com.nestedClass;

import java.util.Scanner;

public class University {
	public static void main(String args[]) {



	      
	       String s1 = "Hello";

	       String s2 = "Hello";

	      

	       if(s1 == s2) {

	           System.out.println("Strings are equal");

	       }

	       else {

	           System.out.println("Strings are not equal");

	       }

	  }
	
}
